<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddWishListActivity">

    <TextView
        android:id="@+id/group_list_page_title"
        android:layout_width="341dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:text="Suggested group for you"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/Group_list"
        android:layout_width="385dp"
        android:layout_height="476dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:divider="@android:color/white"
        android:dividerHeight="2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/group_list_page_title"
        tools:listitem="@android:layout/simple_dropdown_item_1line" />


</androidx.constraintlayout.widget.ConstraintLayout>